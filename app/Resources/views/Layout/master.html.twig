{{ include ('Layout/header.html.twig') }}
{% for success_message in app.session.flashbag.get('delete') %}
    <div class="alert alert-danger">{{ success_message }}</div>
{% endfor %}

 {% for success_message in app.session.flashbag.get('message') %}
     <div class="alert alert-success">{{ success_message }}</div>
 {% endfor %}



<a href="{{ absolute_url(asset('')) }}app_dev.php/insert" class="btn btn-primary ">Create Post</a> <br><br>




<table class="table table-striped table-hover ">
    <thead>
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Phone Number</th>
        <th>City</th>
        <th>Country</th>
        <th>Date OF Birth</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for datas in data %}
    <tr>
        <td>{{ datas.id }}</td>
        <td>{{ datas.name }}</td>
        <td>{{ datas.lastname }}</td>
        <td>{{ datas.address }}</td>
        <td>{{ datas.phonenumber }}</td>
        <td>{{ datas.city }}</td>
        <td>{{ datas.country }}</td>
        <td>{{ datas.dateofbirth }}</td>

        <td>
            <a href="{{ absolute_url(asset('')) }}app_dev.php/view/{{ datas.id }}" class="btn btn-primary btn-xs">View</a>
            <a href="{{ absolute_url(asset('')) }}app_dev.php/edit/{{ datas.id }}" class="btn btn-success btn-xs">Edit</a>
            <a href="{{ absolute_url(asset('')) }}app_dev.php/delete/{{ datas.id }}" class="btn btn-danger btn-xs">Delete</a>

        </td>

    </tr>
    {% endfor %}
    </tbody>
</table>










{{ include ('Layout/footer.html.twig') }}